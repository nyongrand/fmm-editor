<Window xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:vm="using:FMM.ViewModels"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:views="using:FMM.Views"
		WindowStartupLocation="CenterOwner"
		xmlns:ui="using:FluentAvalonia.UI.Controls"
		mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		x:Class="FMM.Views.MainWindow"
		Icon="/Assets/avalonia-logo.ico"
		Title="FMM">
	<Grid ColumnDefinitions="*">

		<ui:NavigationView Grid.Row="0"
						   Name="navigateView"
						   IsPaneToggleButtonVisible="True"
						   IsSettingsVisible="False"
						   PaneDisplayMode="LeftMinimal">

			<ui:NavigationView.MenuItems>
				<ui:NavigationViewItem Content="Competitions"/>
				<ui:NavigationViewItem Content="Clubs"/>
				<ui:NavigationViewItem Content="Nations"/>
				<ui:NavigationViewItem Content="Names"/>
				<ui:NavigationViewItem Content="People"/>
			</ui:NavigationView.MenuItems>
		</ui:NavigationView>
	</Grid>
</Window>
