<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:FMM.ViewModels"
			 x:Class="FMM.Views.MainView"
			 x:DataType="vm:MainViewModel"
			 xmlns:ui="using:FluentAvalonia.UI.Controls"
			 mc:Ignorable="d"
			 d:DesignWidth="1280"
			 d:DesignHeight="720">
	<Design.DataContext>
		<vm:MainViewModel />
	</Design.DataContext>

	<Grid ColumnDefinitions="*">

		<ui:NavigationView Grid.Row="0"
						   Name="navigateView"
						   IsPaneToggleButtonVisible="True"
						   IsSettingsVisible="False"
						   PaneDisplayMode="LeftMinimal"
						   SelectionChanged="NavigationView_SelectionChanged">

			<ui:NavigationView.MenuItems>
				<ui:NavigationViewItem Content="Competitions" Tag="Competitions"/>
				<ui:NavigationViewItem Content="Clubs" Tag="Clubs"/>
				<ui:NavigationViewItem Content="Nations" Tag="Nations"/>
				<ui:NavigationViewItem Content="Names" Tag="Names"/>
				<ui:NavigationViewItem Content="People" Tag="People"/>
			</ui:NavigationView.MenuItems>
		</ui:NavigationView>
	</Grid>
</UserControl>
